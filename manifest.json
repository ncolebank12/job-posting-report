{
    "manifest_version": 3,
    "name": "Job Posting Report",
    "description": "Report job postings that seem fraudulent.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon500.png"
    },
    "permissions": [
      "activeTab",
      "identity",
      "identity.email",
      "tabs"
    ],
    "background": {
      "service_worker": "src/service-worker.ts"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }