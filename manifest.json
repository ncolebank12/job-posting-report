{
    "manifest_version": 3,
    "name": "Job Posting Report",
    "description": "Report job postings that seem fraudulent.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "vite.svg"
    },
    "permissions": [
      "activeTab",
      "identity",
      "identity.email",
      "tabs"
    ],
    "background": {
      "service_worker": "src/service-worker.ts"
    }
  }